            "MIMode": "gdb",
            "miDebuggerPath": "D:\\Dev\\DevKit\\CXX\\mingw64-posix-seh-x64-8.1.0\\bin\\gdb.exe",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
